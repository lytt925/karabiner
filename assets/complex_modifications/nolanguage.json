            "description": "CapsLock to Hyper",
            "manipulators": [
                {
                    "description": "capslock = hyper(hold)",
                    "from": {
                        "key_code": "caps_lock",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_shift",
                            "modifiers": [
                                "right_command",
                                "right_control",
                                "right_option"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "description": "escape = capslock switch",
                    "from": {
                        "key_code": "escape",
                        "modifiers": {
                            "mandatory": [
                                "right_command",
                                "right_control",
                                "right_shift",
                                "right_option"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "caps_lock",
                            "modifiers": [
                                "left_control"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
